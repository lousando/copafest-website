---
const { blok } = Astro.props;

let linkURL = "";

switch (blok.link.linktype) {
  case "url":
  case "asset":
    linkURL = blok.link.url
    break;
  case "story":
    linkURL = blok.link.cached_url;
    break;
  case "email":
    linkURL = `mailto:${blok.link.url}`;
    break;
}
---

<a
  class="navbar-item"
  href={linkURL}
  target={blok.link?.target}
>
  {blok.text}
</a>
